{
  "name": "lb4-mysql",
  "version": "1.0.0",
  "description": "lb4 mysql",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm start"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start:dev": "nodemon . --inspect",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.4.4",
    "@loopback/context": "^1.20.2",
    "@loopback/core": "^1.8.5",
    "@loopback/openapi-v3": "^1.7.0",
    "@loopback/repository": "^1.8.2",
    "@loopback/rest": "^1.16.3",
    "@loopback/rest-explorer": "^1.2.5",
    "loopback-connector-mysql": "^5.4.1"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.3",
    "@loopback/testlab": "^1.6.3",
    "@types/node": "^10.14.10",
    "nodemon": "^1.19.1",
    "source-map-support": "^0.5.12",
    "tsc-watch": "^2.2.1",
    "typescript": "~3.5.2"
  }
}
